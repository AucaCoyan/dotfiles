{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "lavender": "#B7BDF8",
    "blue": "#8AADF4",
    "wight": "#FFFFFF",
    "text": "#494D64"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:wight",
          "type": "os",
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "macos": "\uf179",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "nu"
          }
        },
        {
          "type": "root",
          "style": "diamond",
          "leading_diamond": "<transparent,#DE2121>\ue0b0</>",
          "background": "#DE2121",
          "foreground": "#FFFFFF",
          "template": " \uf0e7 ",
          "trailing_diamond": "\ue0b0"
        },
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "\ue0b0",
          "background": "p:pink",
          "foreground": "p:text",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf6d7",
            "home_icon": "\uf7db",
            "style": "full",
            "folder_separator_icon": "<transparent> \ue0b1 </>"
          }
        },
        {
          "type": "git",
          "background": "p:lavender",
          "foreground": "p:text",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FAB387{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#a6e3a1{{ end }}",
            "{{ if gt .Ahead 0 }}#89b4fa{{ end }}",
            "{{ if gt .Behind 0 }}#89b4fa{{ end }}"
          ],
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UpstreamIcon}} {{ .HEAD }} branch |{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}{{ if gt .Working.Unmerged 0}} unmerged: {{ .Working.Unmerged }}{{ end }}{{ if gt .Working.Added 0}} | added: {{ .Working.Added }}{{ end }}{{ if gt .Working.Deleted 0}} | deleted: {{ .Working.Deleted }}{{ end }}{{ if gt .Working.Modified 0}} | modified: {{ .Working.Modified }}{{ end }}{{ if gt .Working.Untracked 0}} | untracked: {{ .Working.Untracked }}{{ end }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }}{{ end }}{{ if .Staging.Changed }} | staging: \uF046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  saved stashes \uF692 {{ .StashCount }}{{ end }}",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          }
        },
        {
          "type": "python",
          "foreground": "#a6e3a1",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "style": "diamond",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "trailing_diamond": "<transparent> \ue0b1 </>"
        }
      ]
    }
  ],
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.Folder | replace \"~\" \"üè†\"}} @ {{.HostName}}",
  "final_space": true,
  "version": 2
}